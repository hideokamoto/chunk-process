{
  "name": "chunk-process",
  "version": "0.0.0",
  "description": "Chunked parallel async task runner for rate limiting and resource control",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "tsc": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint libs/",
    "lint:fix": "eslint libs/ --fix",
    "doc": "typedoc --out docs libs/ && touch docs/.nojekyll",
    "release": "np",
    "postversion": "git push origin $(git describe --abbrev=0 --tags)",
    "prepare": "husky"
  },
  "keywords": [
    "batch",
    "chunk",
    "async",
    "promise",
    "rate-limit",
    "throttle",
    "parallel",
    "processing"
  ],
  "bugs": {
    "url": "https://github.com/hideokamoto/chunk-process/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideokamoto/chunk-process.git"
  },
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "np": "^10.2.0",
    "tslib": "^2.8.1",
    "typedoc": "^0.27.5",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^2.1.8"
  }
}
